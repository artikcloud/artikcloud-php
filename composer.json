{
    "name": "artikcloud/artikcloud-php",
    "version": "2.0.5",
    "description": "This SDK helps you connect your PHP scripts to ARTIK Cloud. The SDK exposes a number of methods to easily execute REST API calls to ARTIK Cloud",
    "keywords": [
        "php",
        "sdk",
        "api"
    ],
    "license": "Apache v2",
    "authors": [
        {
            "name": "Artik Cloud and contributors",
            "homepage": "https://github.com/artikcloud/artikcloud-php"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "autoload": {
        "psr-4": { "ArtikCloud\\" : "lib/", "ArtikCloudTests\\" : "test/Api" }
    },
    "autoload-dev": {
        "psr-4": { "ArtikCloud\\" : "test/" }
    }
}
